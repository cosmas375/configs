{
    "window.zoomLevel": -1,
    "editor.hover.delay": 500,
    "editor.padding.top": 10,
    "editor.padding.bottom": 20,
    "editor.smoothScrolling": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modifications",
    "editor.minimap.maxColumn": 80,
    "editor.minimap.renderCharacters": false,
    "files.maxMemoryForLargeFilesMB": 1024,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.fontFamily": "monospace",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "css.lint.duplicateProperties": "warning",
    "css.lint.float": "error",
    "css.lint.zeroUnits": "warning",
    "html.format.endWithNewline": true,
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "scss.lint.duplicateProperties": "error",
    "scss.lint.emptyRules": "error",
    "scss.lint.float": "error",
    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "Default Dark+",
    "workbench.iconTheme": "material-icon-theme",
    "html.format.wrapAttributes": "force-expand-multiline",
    "workbench.sideBar.location": "right",
    "editor.scrollBeyondLastColumn": 15,
    "gitlens.blame.heatmap.location": "left",
    "explorer.confirmDragAndDrop": false
}